# Canon R3 Camera Configuration
# Copy this file to .env and update with your camera's credentials

# Camera IP address (usually 192.168.1.2 when connected via WiFi)
CANON_R3_IP=192.168.1.2

# Camera HTTP port (default is 8080)
CANON_R3_PORT=8080

# Camera username (default is admin)
CANON_R3_USERNAME=admin

# Camera password (set this to your actual password)
CANON_R3_PASSWORD=your_password_here

# FTP Server Configuration
FTP_PORT=2121
JPEG_QUALITY=95

# ngrok Configuration (optional - can also use ngrok config)
# NGROK_API_TOKEN=your_ngrok_auth_token_here 